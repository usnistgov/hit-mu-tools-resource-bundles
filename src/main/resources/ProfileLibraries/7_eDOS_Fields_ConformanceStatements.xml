	<!-- MSH for EDOS_GU & EDOS_NG -->
	<ConformanceStatement Profile="Base" id="eDOS-6" Context="MSH.1" flavor="EDOS_GU">
		<EnglishDescription>MSH-1 (Field Separator) SHALL contain the constant value '|'.</EnglishDescription>
		<Assertion>
			<PlainText location="." value="|"/>
		</Assertion>		
	</ConformanceStatement>	
	<ConformanceStatement Profile="Base" id="eDOS-6" Context="MSH.1" flavor="EDOS_NG">
		<EnglishDescription>MSH-1 (Field Separator) SHALL contain the constant value '|'.</EnglishDescription>
		<Assertion>
			<PlainText location="." value="|"/>
		</Assertion>		
	</ConformanceStatement>	
	
	<ConformanceStatement Profile="Base" id="eDOS-7" Context="MSH.2" flavor="EDOS_GU">
		<EnglishDescription>MSH-2 (Encoding Characters) SHALL contain the constant value '^~\&amp;' or the constant value '^~\&amp;#'.</EnglishDescription>
		<Assertion>
			<OR>
				<PlainText location="." value="^~\&amp;#"/>
				<PlainText location="." value="^~\&amp;"/>
			</OR>
		</Assertion>		
	</ConformanceStatement>	
	<ConformanceStatement Profile="Base" id="eDOS-7" Context="MSH.2" flavor="EDOS_NG">
		<EnglishDescription>MSH-2 (Encoding Characters) SHALL contain the constant value '^~\&amp;' or the constant value '^~\&amp;#'.</EnglishDescription>
		<Assertion>
			<OR>
				<PlainText location="." value="^~\&amp;#"/>
				<PlainText location="." value="^~\&amp;"/>
			</OR>
		</Assertion>		
	</ConformanceStatement>	
	
	<ConformanceStatement Profile="Base" id="eDOS-8" Context="MSH.12.1" flavor="EDOS_GU">
