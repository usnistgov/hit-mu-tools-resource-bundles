		<Assertion>
			<Custom className="gov.nist.healthcare.mu.edos.custom.MFI" id="1" />
		</Assertion>		
	</ConformanceStatement>	
	<ConformanceStatement Profile="Base" id="eDOS-11" Context="MFI.1" flavor="EDOS">
