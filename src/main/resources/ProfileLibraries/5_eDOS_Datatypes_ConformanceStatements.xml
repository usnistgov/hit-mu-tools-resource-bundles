	<!-- EI_GU -->
	<ConformanceStatement  Profile="Base" id="eDOS-1" Context="EI_GU.3">
		<EnglishDescription>EI_GU.3 (Universal ID) SHALL be valued with an ISO-compliant OID.</EnglishDescription>
		<Assertion>
			<Regex location="." regex="[0-2](\.(0|[1-9][0-9]*))*"/>
		</Assertion>		
	</ConformanceStatement>
	<ConformanceStatement  Profile="Base" id="eDOS-2" Context="EI_GU.4">
		<EnglishDescription>EI_GU.4 (Universal ID Type) SHALL contain the value 'ISO'.</EnglishDescription>
		<Assertion>
			<PlainText location="." value="ISO"/>
		</Assertion>	
	</ConformanceStatement>
	<!-- HD_GU -->
	<ConformanceStatement  Profile="Base" id="eDOS-3" Context="HD_GU.2">
		<EnglishDescription>HD_GU.2 (Universal ID) SHALL be valued with an ISO-compliant OID.</EnglishDescription>
		<Assertion>
			<Regex location="." regex="[0-2](\.(0|[1-9][0-9]*))*"/>
		</Assertion>		
	</ConformanceStatement>
	<ConformanceStatement  Profile="Base" id="eDOS-4" Context="HD_GU.3">
		<EnglishDescription>HD_GU.3 (Universal ID Type) SHALL contain the value 'ISO'.</EnglishDescription>
		<Assertion>
			<PlainText location="." value="ISO"/>
		</Assertion>		
	</ConformanceStatement>
