					</Assertion>
				</Constraint>
				<!-- PID-5 -->
				<Constraint ID="SS-023" Target="5[1]">
					<Description>PID-5 (Patient Name) SHALL be valued as |~^^^^^^S| or |~^^^^^^U|). </Description>
					<Assertion>
						<FORALL>
							<NOT>
								<Presence Path="5[1]"/>
							</NOT>
							<NOT>
								<Presence Path="5[2].1[1]"/>
